        android:background="@android:color/holo_blue_light"
        android:fontFamily="@font/robotoblack"
        android:text="# CELULAR:"
        android:textSize="18sp"
        app:drawableTint="@color/black"
        app:layout_constraintBottom_toBottomOf="parent"
